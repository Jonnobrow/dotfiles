[SpamFilter]
[KillThreadsFilter]
[ListMailsFilter]
[SentMailsFilter]
sent_tag = sent
[ArchiveSentMailsFilter]

[Filter.1]
message = "Get mailing lists out"
query = 'tag:lists'
tags = -inbox

[Filter.2]
message = "Tag all messages from projects"
query = 'from:gitlab.com or from:github.com or from:sr.ht'
tags = +proj;-inbox;

[Filter.3]
message = "Tag all Sky messages"
query = 'from:sky.uk'
tags = +sky;-inbox;

[Filter.4]
message = "Move emails from unrecognised"
query = 'to:jonnobrow.co.uk and not to:jonathan@jonnobrow.co.uk and not to:jsb@jonnobrow.co.uk'
tags = -inbox;+wildcard;

[InboxFilter]

[MailMover]
folders = mailbox/INBOX
rename = True
max_age = 60

mailbox/INBOX = 'tag:spam':mailbox/Junk 'NOT tag:inbox AND tag:sky':mailbox/INBOX/Sky 'NOT tag:inbox AND tag:proj':mailbox/INBOX/Projects 'NOT tag:inbox AND tag:wildcard':mailbox/INBOX/Wildcard 'NOT tag:inbox AND tag:lists':mailbox/INBOX/Lists



