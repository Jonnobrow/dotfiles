[calendars]

[[personal]]
path = ~/.local/share/calendar/nextcloud/personal
type = calendar
color = "light green"

[[birthdays]]
path = ~/.local/share/calendar/nextcloud/contact_birthdays
type = birthdays
color = "light blue"

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M

[default]
default_calendar = personal
highlight_event_days = True
