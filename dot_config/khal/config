[calendars]

[[personal]]
path = ~/.local/share/calendar/nextcloud/*
type = discover
color = auto

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M

[default]
default_calendar = personal
highlight_event_days = True

[keybindings]
external_edit = x
